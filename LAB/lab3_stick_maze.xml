      <!-- === LAB 3 OBJECTS: Stick Maze === -->

    <!-- Maze built from fixed walls on the table -->
        <!-- Maze built from a mesh -->
    <!-- Replace the old maze body with this -->
        <!-- Maze built from one mesh sitting on the table -->
    <body name="maze" pos="0.6 0 0.425">  <!-- z = table height -->
      <!-- purely visual mesh -->
      <geom name="maze_visual"
            type="mesh"
            mesh="maze_mesh"
            rgba="0.4 0.4 0.4 1"
            contype="0"
            conaffinity="0"/>

      <!-- invisible collision mesh, same geometry -->
            <geom name="maze_collision"
            type="mesh"
            mesh="maze_mesh"
            rgba="0 0 1 0"
            contype="1"
            conaffinity="1"
            friction="1.0 0.005 0.0001"
            solref="0.02 1"
            solimp="0.95 0.99 0.001"
            margin="0.004"/>
    </body>

    <!-- Stick using OBJ mesh -->
        <!-- Stick with custom mesh -->
    <body name="stick" pos="0.5 -0.2 0.70" >
      <joint name="stick_free" type="free"/>

      <!-- Visual mesh -->
      <geom name="stick_visual"
            type="mesh"
            mesh="stick_mesh"
            rgba="0.8 0.8 0.2 1"
            contype="0"
            conaffinity="0"/>

      <!-- Collision mesh -->
            <!-- Collision primitive (more stable than mesh contact) -->
      <geom name="stick_collision"
            type="capsule"
            fromto="-0.15 0 0  0.15 0 0" size="0.01"
            rgba="0 0 0 0"
            contype="1"
            conaffinity="1"
            friction="1.2 0.01 0.0001"
            solref="0.01 1"
            solimp="0.9 0.95 0.001"
            margin="0.002" mass="0.2"/>
            <!-- top -->
      <geom type="capsule"
            fromto="-0.03  0.03 0   0.03  0.03 0"
            rgba="0 0 0 0"
            size="0.002"/>

      <!-- bottom -->
      <geom type="capsule"
            fromto="-0.03 -0.03 0   0.03 -0.03 0"
            rgba="0 0 0 0"
            size="0.002"/>

      <!-- left -->
      <geom type="capsule"
            fromto="-0.03 -0.03 0  -0.03  0.03 0"
            rgba="0 0 0 0"
            size="0.002"/>

      <!-- right -->
      <geom type="capsule"
            fromto=" 0.03 -0.03 0   0.03  0.03 0"
            rgba="0 0 0 0"
            size="0.002"/>

<!-- make collision mesh invisible too -->
    </body>
      <!-- Ground -->
    <geom name="ground" type="plane" size="2 2 0.1" pos="0 0 0" rgba="0.3 0.3 0.3 1"/>

    <!-- Table -->
